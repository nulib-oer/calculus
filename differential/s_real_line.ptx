<definition xml:id="d_abs_value">
  <title>Absolute value</title>

  <statement>
    <p>
      <me>
        \lvert x\rvert=\begin{cases} x \amp \text{if } x\geq 0\\
                            -x \amp \text{if } x<0
                            \end{cases}
      </me>.

    </p>
  </statement>
</definition>
